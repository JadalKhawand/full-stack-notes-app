export const createNote = async (req: Request, res: Response) => {
  const { title, content } = req.body;
  const userId = req.body.id
  const user = await prisma.note.create({
    data: {
      title: title,
      content: content,
      userId: userId
    },
  });
  console.log(user);
  res.json(user);
};